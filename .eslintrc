{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "object-curly-newline": ["error", { "multiline": true }],
    "arrow-parens": [2, "always"],
    "jsx-a11y/label-has-associated-control": [ 2, {
      "assert": "either"
    }]
  },
    "env": {
      "browser": true,
      "node": true
    }
}
